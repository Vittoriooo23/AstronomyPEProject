      </CheckBox>
      <CheckBox fx:id="SunCB" layoutX="351.0" layoutY="569.0" mnemonicParsing="false" onAction="#SunSelected" prefHeight="68.0" prefWidth="294.0" text="Sun" textFill="WHITE">
