      </ImageView>
      <Label fx:id="WhichPlanetLabel" layoutX="144.0" layoutY="209.0" prefHeight="111.0" prefWidth="414.0" text="Which Planet?" textFill="WHITE" AnchorPane.leftAnchor="280.0">
         <font>
            <Font size="64.0" />
         </font>
      </Label>
      <CheckBox fx:id="EarthCB" layoutX="351.0" layoutY="352.0" mnemonicParsing="false" onAction="#EarthSelected" prefHeight="68.0" prefWidth="294.0" text="Earth" textFill="WHITE">
         <font>
            <Font size="48.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="MarsCB" layoutX="351.0" layoutY="461.0" mnemonicParsing="false" onAction="#MarsSelected" prefHeight="68.0" prefWidth="294.0" text="Mars" textFill="WHITE">
