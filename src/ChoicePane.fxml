      <Button fx:id="Science" layoutX="679.0" layoutY="723.0" mnemonicParsing="false" onAction="#Begin" prefHeight="83.0" prefWidth="242.0" style="-fx-background-color: white;" text="Science!">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
   </children>
