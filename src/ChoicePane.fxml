      </CheckBox>
      <CheckBox fx:id="AsteroidCB" layoutX="964.0" layoutY="461.0" mnemonicParsing="false" onAction="#AsteroidSelected" prefHeight="68.0" prefWidth="294.0" text="Asteroid" textFill="WHITE">
